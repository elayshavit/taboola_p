      "components/layout/sidebar.tsx",
      "components/layout/app-shell.tsx",
      "store/use-companies-store.ts",
      "store/use-company-store.ts",
      "store/use-compare-store.ts",
      "data/data.ts",
      "data/companiesMetrics.ts",
      "lib/adapter.ts",
      "lib/schema.ts",
      "lib/normalize.ts",
